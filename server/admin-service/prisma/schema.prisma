datasource db {
  provider = "postgresql" 
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}


model SubscriptionPlan {
  id        String   @id @default(uuid()) 
  name      String   @unique
  price     Float
  duration  Int      
  features  Json     
  status    String   @default("active") 
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
